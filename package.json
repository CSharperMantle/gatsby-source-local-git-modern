{
  "name": "gatsby-source-local-git",
  "version": "1.1.1",
  "description": "Gatsby source plugin which sources data from your local git repository",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "git"
  ],
  "homepage": "https://github.com/PMudra/gatsby-source-local-git#readme",
  "bugs": "https://github.com/PMudra/gatsby-source-local-git/issues",
  "repository": "github:PMudra/gatsby-source-local-git",
  "author": "Philipp Mudra <philipp.mudra@web.de>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:build",
    "build": "tsc",
    "watch:build": "npm run build -- --watch",
    "prepack": "npm run test && npm run build",
    "test": "jest --coverage",
    "watch:test": "jest --watch",
    "changelog": "npx keep-a-changelog"
  },
  "dependencies": {
    "simple-git": "^2.36.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/react-dom": "^17.0.0",
    "gatsby": "^2.32.9",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "*.js",
    "dist/**/*.js"
  ]
}
